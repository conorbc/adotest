        <script>
            (function () {
                
// Used by LightBox and "carousel" type galleries
var Carousel = function (props) {

    // DOM stuff
    this.$carouselWrapper = props.$gallery.querySelector('.bc-carousel-wrapper');
    this.$controls = props.$gallery.querySelector('.bc-carousel-controls-container');
    this.$slideCover = props.$gallery.querySelector('.slide-cover');
    this.$slideA = this.$carouselWrapper.querySelector(':scope > span:nth-child(1) .slide');
    this.$slideB = this.$carouselWrapper.querySelector(':scope > span:nth-child(2) .slide');
    this.$slideC = this.$carouselWrapper.querySelector(':scope > span:nth-child(3) .slide');
    this.$slideD = this.$carouselWrapper.querySelector(':scope > span:nth-child(4) .slide');
    this.$slideE = this.$carouselWrapper.querySelector(':scope > span:nth-child(5) .slide');
    this.$prev = this.$controls && this.$controls.querySelector('.prev.bc-carousel-btn-wrapper');
    this.$middle = this.$controls && this.$controls.querySelector('.mid.bc-carousel-btn-wrapper');
    this.$next = this.$controls && this.$controls.querySelector('.next.bc-carousel-btn-wrapper');

    // State
    this.itemImages = $bc.GALLERY_IMAGES[props.galleryId];
    this.index = props.index || 0;
    this.nextIndex = props.index || 0;
    this.isSliding = false;
    this.width = props.$gallery.getBoundingClientRect().width;
    this.swipeThreshold = this.width / 2;
    this.swipeDirection = 'left';

    // Handlers
    this.updateSlides = this.updateSlides.bind(this);
    this.handleGoToPrevious = this.handleGoToPrevious.bind(this);
    this.handleGoToNext = this.handleGoToNext.bind(this);
    this.handleTouchEnd = this.handleTouchEnd.bind(this);
    this.controlRegion = this.$controls && ZingTouch.Region(this.$controls, false, false);

    // Init
    this.setBgByIndex(this.$slideCover.querySelector('.slide'), this.index);
    this.setSlides(this.index);

    if (this.itemImages.length > 1) {
        $bc.removeClass(this.$controls, 'no-arrows');
        this.addEventListeners();
    }
};

Carousel.prototype.getPrevIndex = function (index) {
    return index === 0 ? this.itemImages.length - 1 : index - 1;
};

Carousel.prototype.getNextIndex = function (index) {
    return (index + 1) % this.itemImages.length;
};

Carousel.prototype.getDirection = function (e) {

    var angle = e.detail.data[0].directionFromOrigin || e.detail.data[0].currentDirection;

    switch (true) {
        case (angle >= 315 && angle <= 360) || (angle <= 45 && angle >= 0):
            return 'right';
        case (angle >= 135 && angle <= 225):
            return 'left';
        default:
            return null;
    }
};

Carousel.prototype.setBgByIndex = function ($slide, index) {
    $slide.style.backgroundImage = 'url("' + this.itemImages[index] + '")';
};

Carousel.prototype.setSlides = function (index) {
    this.setBgByIndex(this.$slideA, this.getPrevIndex(this.getPrevIndex(index)));
    this.setBgByIndex(this.$slideB, this.getPrevIndex(index));
    this.setBgByIndex(this.$slideC, index);
    this.setBgByIndex(this.$slideD, this.getNextIndex(index));
    this.setBgByIndex(this.$slideE, this.getNextIndex(this.getNextIndex(index)));
};

Carousel.prototype.slideToNeighbor = function (targetSlide, isSwiping) {
    if (!this.isSliding) {
        this.isSliding = true;
        this.$carouselWrapper.style.transform = '';
        this.nextIndex = (targetSlide === 3) ? this.getNextIndex(this.index) : this.getPrevIndex(this.index);
        $bc.addClass(this.$slideCover, 'is-sliding');
        this.setBgByIndex(this.$slideCover.querySelector('.slide'), this.nextIndex);
        $bc.removeClass(this.$carouselWrapper, 'dormant');
        $bc.removeClass(this.$carouselWrapper, 'position-2');
        if (isSwiping) {
            $bc.addClass(this.$carouselWrapper, 'is-swipe-transition');
        }
        $bc.addClass(this.$carouselWrapper, 'position-' + targetSlide);
    }
};

Carousel.prototype.updateSlides = function () {
    this.isSliding = false;
    this.index = this.nextIndex;
    $bc.removeClass(this.$slideCover, 'is-sliding');
    $bc.addClass(this.$carouselWrapper, 'dormant');
    $bc.addClass(this.$carouselWrapper, 'position-2');
    $bc.removeClass(this.$carouselWrapper, 'position-1');
    $bc.removeClass(this.$carouselWrapper, 'position-3');
    $bc.removeClass(this.$carouselWrapper, 'is-swipe-transition');
    this.setSlides(this.index);
};

Carousel.prototype.addEventListeners = function () {

    this.$carouselWrapper.addEventListener('transitionend', this.updateSlides);

    if ($bc.isIE) {
        this.$prev.addEventListener('click', this.handleGoToPrevious);
        this.$middle.addEventListener('click', this.handleGoToNext);
        this.$next.addEventListener('click', this.handleGoToNext);
    } else {
        this.controlRegion.bind(this.$controls, 'pan', this.handlePan.bind(this));
        this.controlRegion.bind(this.$controls, 'swipe', this.handleSwipe.bind(this));
        this.controlRegion.bind(this.$prev, 'tap', this.handleGoToPrevious);
        this.controlRegion.bind(this.$middle, 'tap', this.handleGoToNext);
        this.controlRegion.bind(this.$next, 'tap', this.handleGoToNext);
        this.$controls.addEventListener('touchend', this.handleTouchEnd);
        this.$controls.addEventListener('mouseup', this.handleTouchEnd);
        this.$controls.addEventListener('mouseleave', this.handleTouchEnd);
    }
};

Carousel.prototype.removeEventListeners = function () {

    this.$carouselWrapper.removeEventListener('transitionend', this.updateSlides);

    if ($bc.isIE) {
        this.$prev.removeEventListener('click', this.handleGoToPrevious);
        this.$middle.removeEventListener('click', this.handleGoToNext);
        this.$next.removeEventListener('click', this.handleGoToNext);
    } else {
        this.controlRegion.unbind(this.$controls, 'pan');
        this.controlRegion.unbind(this.$controls, 'swipe');
        this.controlRegion.unbind(this.$prev, 'tap');
        this.controlRegion.unbind(this.$middle, 'tap');
        this.controlRegion.unbind(this.$next, 'tap');
        this.$controls.removeEventListener('touchend', this.handleTouchEnd);
        this.$controls.removeEventListener('mouseup', this.handleTouchEnd);
        this.$controls.removeEventListener('mouseleave', this.handleTouchEnd);
    }
};

Carousel.prototype.handleGoToPrevious = function () {
    this.slideToNeighbor(1);
};

Carousel.prototype.handleGoToNext = function () {
    this.slideToNeighbor(3);
};

Carousel.prototype.handlePan = function (e) {

    if (this.isSliding) return;

    this.swipeDirection = this.getDirection(e);

    if (!$bc.isDesktopSafari && !$bc.isIE && (this.swipeDirection === 'right' || this.swipeDirection === 'left')) {

        var distanceFromOrigin = e.detail.data[0].distanceFromOrigin;
        var modifier = this.swipeDirection === 'right' ? 1 : -1;
        var percentSwiped = ((distanceFromOrigin / (this.width * 5)) * 100) * modifier;
        var transform = 'translateX(' + (-40 + percentSwiped) + '%)';

        $bc.addClass(this.$slideCover, 'is-sliding');
        $bc.addClass(this.$carouselWrapper, 'dormant');
        this.$carouselWrapper.style.transform = transform;

        this.shouldSwipe = distanceFromOrigin > this.swipeThreshold;
    }
};

Carousel.prototype.handleSwipe = function (e) {

    this.swipeDirection = this.getDirection(e);
    if (this.swipeDirection === 'right' || this.swipeDirection === 'left') {
        var direction = (this.swipeDirection === 'right') ? 1 : 3;
        this.slideToNeighbor(direction, true);
        // Swipe handled
        this.shouldSwipe = false;
    }
};

Carousel.prototype.handleTouchEnd = function () {

    $bc.removeClass(this.$carouselWrapper, 'dormant');

    if (this.$carouselWrapper.style.transform.length) {
        $bc.addClass(this.$carouselWrapper, 'is-swipe-transition');
    }

    this.$carouselWrapper.style.transform = '';

    if (!this.shouldSwipe) return;

    if (this.swipeDirection === 'left') {
        this.slideToNeighbor(3, this.shouldSwipe);
    } else {
        this.slideToNeighbor(1, this.shouldSwipe);
    }

    this.shouldSwipe = false;
};

                
// Used for LightBox, as those elements remain in the DOM,
// but can be used for multiple galleries.
var $lightbox = document.querySelector('.lightbox-container');
var $closeBtn = $lightbox.querySelector('.bc-carousel-close');

// Used by "layout", "masonry" type galleries. Items will open lightbox
var GalleryItem = function (props) {
    this.props = props;
    props.$el.addEventListener('click', this.handleClick.bind(this));
};

GalleryItem.prototype.handleClick = function () {
    this.props.enableLightBox(this.props.index);
};

// Used by "layout", "masonry" type galleries.
var Gallery = function ($gallery) {

    var _this = this;
    var $galleryItems = $gallery.querySelectorAll('.bc-gallery-item-wrapper');

    this.$gallery = $gallery;
    this.arrowEvents = $galleryItems.length > 1;

    // Init Gallery Items
    for (var index = 0; index < $galleryItems.length; index++) {
                var galleryItem = new GalleryItem({
            index: index,
            $el: $galleryItems[index],
            enableLightBox: _this.enableLightBox.bind(_this)
        });
    }
};

Gallery.prototype.enableLightBox = function (index) {

    this.carousel = new Carousel({
        $gallery: $lightbox,
        galleryId: this.$gallery.getAttribute('data-uuid'),
        index: index,
    });

    // Handlers
    this.disableLightbox = this.disableLightbox.bind(this);
    this.handleKeyDown = this.handleKeyDown.bind(this);

    // Init
    $bc.addClass($lightbox, 'active');
    this.addEventListeners();
};

Gallery.prototype.disableLightbox = function () {
    $bc.removeClass($lightbox, 'active');
    this.carousel.removeEventListeners();
    this.removeEventListeners();
};

Gallery.prototype.addEventListeners = function () {
    window.addEventListener('keydown', this.handleKeyDown);
    $closeBtn.addEventListener('click', this.disableLightbox);
};

Gallery.prototype.removeEventListeners = function () {
    window.removeEventListener('keydown', this.handleKeyDown);
    $closeBtn.removeEventListener('click', this.disableLightbox);
};

Gallery.prototype.handleKeyDown = function (event) {
    switch (event.keyCode) {
        // Esc
        case 27:
            this.disableLightbox();
            break;
        // Left
        case 37:
            if (this.arrowEvents) {
                Carousel.prototype.handleGoToPrevious.call(this.carousel);
            }
            break;
        // Right
        case 39:
            if (this.arrowEvents) {
                Carousel.prototype.handleGoToNext.call(this.carousel);
            }
            break;
        default:
            // No default behavior
    }
};

                
// Every gallery on the page
var $galleries = document.querySelectorAll('.bc-gallery-wrapper');

var initGallery = function ($gallery) {
    if ($bc.hasClass($gallery, 'carousel')) {
        return new Carousel({
            $gallery: $gallery,
            galleryId: $gallery.getAttribute('data-uuid')
        });
    }
    return new Gallery($gallery);
};

// Init galleries...
for (var i = 0; i < $galleries.length; i++) {
    initGallery($galleries[i]);
}

            }());
