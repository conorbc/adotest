Carousel.prototype.handlePan = function (e) {

    if (this.isSliding) return;

    this.swipeDirection = this.getDirection(e);

    if (!$bc.isDesktopSafari && !$bc.isIE && (this.swipeDirection === 'right' || this.swipeDirection === 'left')) {

        var distanceFromOrigin = e.detail.data[0].distanceFromOrigin;
        var modifier = this.swipeDirection === 'right' ? 1 : -1;
        var percentSwiped = ((distanceFromOrigin / (this.width * 5)) * 100) * modifier;
        var transform = 'translateX(' + (-40 + percentSwiped) + '%)';

        $bc.addClass(this.$slideCover, 'is-sliding');
        $bc.addClass(this.$carouselWrapper, 'dormant');
        this.$carouselWrapper.style.transform = transform;

        this.shouldSwipe = distanceFromOrigin > this.swipeThreshold;
    }
};

Carousel.prototype.handleSwipe = function (e) {

    this.swipeDirection = this.getDirection(e);
    if (this.swipeDirection === 'right' || this.swipeDirection === 'left') {
        var direction = (this.swipeDirection === 'right') ? 1 : 3;
        this.slideToNeighbor(direction, true);
        // Swipe handled
        this.shouldSwipe = false;
    }
};

Carousel.prototype.handleTouchEnd = function () {

    $bc.removeClass(this.$carouselWrapper, 'dormant');

    if (this.$carouselWrapper.style.transform.length) {
        $bc.addClass(this.$carouselWrapper, 'is-swipe-transition');
    }

    this.$carouselWrapper.style.transform = '';

    if (!this.shouldSwipe) return;

    if (this.swipeDirection === 'left') {
        this.slideToNeighbor(3, this.shouldSwipe);
    } else {
        this.slideToNeighbor(1, this.shouldSwipe);
    }

    this.shouldSwipe = false;
};

                
// Used for LightBox, as those elements remain in the DOM,
// but can be used for multiple galleries.
var $lightbox = document.querySelector('.lightbox-container');
var $closeBtn = $lightbox.querySelector('.bc-carousel-close');

// Used by "layout", "masonry" type galleries. Items will open lightbox
var GalleryItem = function (props) {
    this.props = props;
    props.$el.addEventListener('click', this.handleClick.bind(this));
};

GalleryItem.prototype.handleClick = function () {
    this.props.enableLightBox(this.props.index);
};

// Used by "layout", "masonry" type galleries.
var Gallery = function ($gallery) {

    var _this = this;
    var $galleryItems = $gallery.querySelectorAll('.bc-gallery-item-wrapper');

    this.$gallery = $gallery;
    this.arrowEvents = $galleryItems.length > 1;

    // Init Gallery Items
    for (var index = 0; index < $galleryItems.length; index++) {
                var galleryItem = new GalleryItem({
            index: index,
            $el: $galleryItems[index],
            enableLightBox: _this.enableLightBox.bind(_this)
        });
    }
};

Gallery.prototype.enableLightBox = function (index) {

    this.carousel = new Carousel({
        $gallery: $lightbox,
        galleryId: this.$gallery.getAttribute('data-uuid'),
        index: index,
    });

    // Handlers
    this.disableLightbox = this.disableLightbox.bind(this);
    this.handleKeyDown = this.handleKeyDown.bind(this);

    // Init
    $bc.addClass($lightbox, 'active');
    this.addEventListeners();
};

Gallery.prototype.disableLightbox = function () {
    $bc.removeClass($lightbox, 'active');
    this.carousel.removeEventListeners();
    this.removeEventListeners();
};

Gallery.prototype.addEventListeners = function () {
    window.addEventListener('keydown', this.handleKeyDown);
    $closeBtn.addEventListener('click', this.disableLightbox);
};

Gallery.prototype.removeEventListeners = function () {
    window.removeEventListener('keydown', this.handleKeyDown);
    $closeBtn.removeEventListener('click', this.disableLightbox);
};

Gallery.prototype.handleKeyDown = function (event) {
    switch (event.keyCode) {
        // Esc
        case 27:
            this.disableLightbox();
            break;
        // Left
        case 37:
            if (this.arrowEvents) {
                Carousel.prototype.handleGoToPrevious.call(this.carousel);
            }
            break;
        // Right
        case 39:
            if (this.arrowEvents) {
                Carousel.prototype.handleGoToNext.call(this.carousel);
            }
            break;
        default:
            // No default behavior
    }
};

                
// Every gallery on the page
var $galleries = document.querySelectorAll('.bc-gallery-wrapper');

var initGallery = function ($gallery) {
    if ($bc.hasClass($gallery, 'carousel')) {
        return new Carousel({
            $gallery: $gallery,
            galleryId: $gallery.getAttribute('data-uuid')
        });
    }
    return new Gallery($gallery);
};

// Init galleries...
for (var i = 0; i < $galleries.length; i++) {
    initGallery($galleries[i]);
}

            }());
        </script>
    
    
